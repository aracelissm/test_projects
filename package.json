  "author": "Damodharan S",
  "description": "Viper 2.0 Angular UI",
  "scripts": {
    "ng": "node --max_old_space_size=8096 ./node_modules/.bin/ng",
    "start": "ng serve",
    "build": "ng build",
    "build:devdwp": "npm run clean && ng build --configuration=devdwp",
    "build:devips": "npm run clean && ng build --configuration=devips",
    "build:dev": "npm run clean && ng build --configuration=dev",
    "build:qa": "npm run clean && ng build --configuration=qa",
    "build:qa2": "npm run clean && ng build --configuration=qa2",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "tsc --noEmit && eslint . --ext js,ts,json --quiet --fix"
  },
  "private": true,
